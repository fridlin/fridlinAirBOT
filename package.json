{
  "name": "nodejs-starter",
  "version": "2.0.6",
  "description": "A simple Node.js starter project",
  "main": "src/bot.js",
  "scripts": {
    "start": "node src/bot.js",
    "dev": "node --watch src/bot.js",
    "bump:patch": "npm version patch --no-git-tag-version",
    "bump:minor": "npm version minor --no-git-tag-version",
    "bump:major": "npm version major --no-git-tag-version",
    "postversion": "echo $npm_package_version > VERSION && node scripts/generate-readme.js",
    "release": "node scripts/release.js",
    "changelog": "standard-version --release-as",
    "changelog:patch": "standard-version --skip.tag --skip.commit --skip.bump",
    "changelog:minor": "standard-version --skip.tag --skip.commit --skip.bump",
    "changelog:major": "standard-version --skip.tag --skip.commit --skip.bump",
    "standard-version": "^9.5.0"
  },
  "keywords": [
    "nodejs",
    "starter"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@telegraf/session": "^2.0.0-beta.7",
    "axios": "^1.13.2",
    "express": "^4.18.2",
    "telegraf": "^4.16.3",
    "tz-lookup": "^6.1.25"
  },
  "devDependencies": {
    "standard-version": "^9.5.0"
  }
}
